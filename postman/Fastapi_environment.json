{
	"id": "codementorx-fastapi-environment",
	"name": "CodementorX FastAPI - Local Development",
	"values": [
		{
			"key": "fastapi_base_url",
			"value": "http://127.0.0.1:8001",
			"type": "default",
			"enabled": true,
			"description": "Base URL for the FastAPI Chatbot API"
		},
		{
			"key": "access_token",
			"value": "",
			"type": "secret",
			"enabled": true,
			"description": "JWT Access Token from Django auth (import from Django collection)"
		},
		{
			"key": "conversation_id",
			"value": "",
			"type": "default",
			"enabled": true,
			"description": "Active conversation ID (auto-generated by FastAPI)"
		},
		{
			"key": "model_name",
			"value": "gpt-4o-mini",
			"type": "default",
			"enabled": true,
			"description": "Default AI model to use for chat requests"
		},
		{
			"key": "test_user_id",
			"value": "",
			"type": "default",
			"enabled": true,
			"description": "Test user ID for verification"
		},
		{
			"key": "last_response_tokens",
			"value": "",
			"type": "default",
			"enabled": true,
			"description": "Token usage from last AI response"
		},
		{
			"key": "performance_threshold_ms",
			"value": "30000",
			"type": "default",
			"enabled": true,
			"description": "Maximum acceptable response time for AI requests (30s)"
		},
		{
			"key": "max_context_messages",
			"value": "10",
			"type": "default",
			"enabled": true,
			"description": "Maximum number of context messages to send"
		},
		{
			"key": "default_temperature",
			"value": "0.7",
			"type": "default",
			"enabled": true,
			"description": "Default temperature for AI responses"
		},
		{
			"key": "default_max_tokens",
			"value": "1000",
			"type": "default",
			"enabled": true,
			"description": "Default maximum tokens for AI responses"
		}
	],
	"_postman_variable_scope": "environment"
}